// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Activity {
  id    Int    @id @default(autoincrement())
  name String
  postId Int
  NumberId Int @unique
}

model Place {
  id    Int    @id @default(autoincrement())
  name String

}

model Number {
  id    Int    @id @default(autoincrement())
  name Decimal
  activityID Int @unique

}
